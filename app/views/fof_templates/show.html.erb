<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Fof #<%= @fof.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/fofs" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/fofs/<%= @fof.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit fof
        </a>
      </div>

      <div class="col">
        <a href="/delete_fof/<%= @fof.id %>" class="btn btn-block btn-outline-secondary">
          Delete fof
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Name
      </dt>
      <dd>
        <%= @fof.name %>
      </dd>

      <dt>
        Scale
      </dt>
      <dd>
        <%= @fof.scale %>
      </dd>

      <dt>
        Expiration
      </dt>
      <dd>
        <%= @fof.expiration %>
      </dd>

      <dt>
        User
      </dt>
      <dd>
        <%= @fof.user_id %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@fof.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@fof.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

